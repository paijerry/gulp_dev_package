{
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "$": true,
    "jquery": true
    },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": ["error", {"destructuring": "any"}],
    "func-names": "off",
    "consistent-return": "off",
    "arrow-body-style": "off",
    "no-use-before-define": "off",
  },
  "settings": {
    "import/resolver": {
      "webpack": "webpack.config.js"
    }
  }
}
